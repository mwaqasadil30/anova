{
	"@type": "MessageCard",
	"@context": "http://schema.org/extensions",
	"themeColor": "0076D7",
	"summary": "E2E Test Run Completed",
	"sections": [
		{
			"activityTitle": "**Frontend E2E Test Suite #{CompletionStatus}#**",
			"activityImage": "#{CompletionStatusIconURL}#",
			"facts": [
				{
					"name": "Build",
					"value": "[#{Build.BuildNumber}#](#{System.TeamFoundationCollectionUri}##{System.TeamProject}#/_build/results?buildId=#{Build.BuildId}#&view=ms.vss-test-web.build-test-results-tab)"
				},
				{
					"name": "Failing Tests",
					"value": "#{NumberOfFailingTests}#"
				}
			],
			"markdown": true
		}
	]
}