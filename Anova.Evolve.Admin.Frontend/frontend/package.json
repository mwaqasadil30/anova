  "scripts": {
    "start": "HTTPS=true env REACT_APP_GIT_SHA=`git rev-parse --short HEAD` yarn start-base",
    "open": "node_modules/.bin/cypress open",
    "build": "CI=false react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen --testPathIgnorePatterns=src/e2e",
    "test-ci": "CI=true react-scripts test --env=jest-environment-jsdom-sixteen --reporters=default --reporters=jest-junit --testPathIgnorePatterns=src/e2e --testPathIgnorePatterns=src/api/admin/api.ts --coverage --coverageDirectory=coverage",
    "eject": "react-scripts eject",
    "start-base": "node --max_old_space_size=4096 node_modules/.bin/react-scripts start",
    "type-check": "tsc --noEmit",
    "type-check-watch": "tsc --noEmit --watch",
    "lint": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx src",
    "lint-check": "eslint --cache --ext .js,.jsx,.ts,.tsx src",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,scss,css,md}' --ignore-path './.eslintignore'",
    "format-check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,scss,css,md}' --ignore-path './.eslintignore'",
    "i18n": "tsc --outDir build-locale --target ES5 --jsx preserve --noEmit false && i18next-scanner --config i18next-scanner.config.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "ci-checks-base": "yarn type-check && wait && yarn format-check && wait && yarn lint-check && wait && CI=true yarn test",
    "ci-checks-fix-base": "yarn type-check && wait && yarn format && wait && yarn lint && wait && CI=true yarn test",
    "ci-checks": "CI=true yarn ci-checks-base",
    "ci-checks-fix": "CI=true yarn ci-checks-fix-base"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
